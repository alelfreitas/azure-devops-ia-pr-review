{
    "manifestVersion": 1,
    "id": "OPENAIPRReview",
    "name": "GPT Pull Request Review",
    "version": "0.0.5",
    "publisher": "JFTech",
    "galleryFlags": [
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tarefa para revisar o Pull Request usando GPT model da OpenAI",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"        
    },
    "tags": [
        "OpenAI",
        "GPT",
        "pullRequest",
        "review"
    ],
    "files": [
        {
            "path": "OPENAIPRReview"
        }
    ],
    "repository" : {
        "type" : "git",
        "uri" : "https://github.com/alelfreitas/azure-devops-ia-pr-review"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "OPENAIPRReview",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "OPENAIPRReview"
            }
        }
    ]
